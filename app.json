{
  "addons": [
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": {
      "value": "config.settings.production"
    },
    "DJANGO_SECRET_KEY": {
      "description": "Django's secret key",
      "generator": "secret"
    },
    "IS_REVIEW": {
      "value": "True"
    },
    "DJANGO_AWS_ACCESS_KEY_ID": {
      "value": "awsaccesskey"
    },
    "DJANGO_AWS_SECRET_ACCESS_KEY": {
      "value": "awssecretaccesskey"
    },
    "DJANGO_AWS_STORAGE_BUCKET_NAME": {
      "value": "awsbucketname"
    },
    "DJANGO_ADMIN_URL": {
      "value": "admin-test/"
    },
    "SENTRY_DSN": {
      "value": ""
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "name": "dear-petition",
  "scripts": {},
  "stack": "container"
}
